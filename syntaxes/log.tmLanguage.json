{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "Log",
	"scopeName": "code.log",
	"patterns": [
		{
			"include": "#gcc"
		},
		{
			"include": "#clang"
		},
		{
			"include": "#msvc"
		},
		{
			"include": "#cmake"
		},
		{
			"include": "#iar"
		}
	],
	"repository": {
		"gcc": {
			"patterns": [
				{
					"match": "error: .*$",
					"name": "markup.other.log.error"
				},
				{
					"match": "undefined reference to `.*'",
					"name": "markup.other.log.error"
				},
				{
					"match": "multiple definition of `.*'",
					"name": "markup.other.log.error"
				},
				{
					"match": "warning: .*$",
					"name": "markup.other.log.warn"
				},
				{
					"match": "note: .*$",
					"name": "markup.other.log.info"
				},
				{
					"begin": "In function ",
					"end": ":",
					"captures": {
						"0": {
							"name": "markup.other.log.info"
						}
					},
					"contentName": "meta.embedded.block.cpp",
					"patterns": [
						{
							"include": "source.cpp"
						}
					]
				}
			]
		},
		"clang": {
			"patterns": []
		},
		"msvc": {
			"patterns": [
				{
					"match": "error C[0-9]+: .*$",
					"name": "markup.other.log.error"
				},
				{
					"match": "error LNK[0-9]+: .*$",
					"name": "markup.other.log.error"
				},
				{
					"match": "warning C[0-9]+: .*$",
					"name": "markup.other.log.warn"
				},
				{
					"match": "note: .*$",
					"name": "markup.other.log.info"
				}
			]
		},
		"cmake": {
			"patterns": [
				{
					"match": "^\\[[A-Za-z]+\\]",
					"name": "markup.other.log.debug"
				},
				{
					"match": "\\[[0-9]+/[0-9]+\\]",
					"name": "markup.other.log.highlight"
				},
				{
					"match": "FAILED:",
					"name": "markup.other.log.error"
				}
			]
		},
		"iar": {
			"patterns": [
				{
					"match": "\".+:\\d+\"",
					"name": "markup.other.log.debug"
				},
				{
					"match": "(Fatal )?[Ee]rror\\[.+\\]:.+",
					"name": "markup.other.log.error"
				},
				{
					"match": "~\\s+.+",
					"name": "markup.other.log.warn_weak"
				},
				{
					"match": "!\\s+.+",
					"name": "markup.other.log.error_weak"
				},
				{
					"match": "Error\\[.+\\]:.+",
					"name": "markup.other.log.error"
				},
				{
					"match": "Warning\\[.+\\]:.+",
					"name": "markup.other.log.warn"
				},
				{
					"match": "\\s\\s[\\s]*\\^$",
					"name": "markup.other.log.info"
				}
			]
		}
	}
}